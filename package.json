{
  "name": "poc-servico",
  "version": "1.0.0",
  "description": "Email dashboard service ",
  "main": "index.js",
  "scripts": {
    "dev": "tsc && serverless invoke local -f handler",
    "lint": "eslint src/**/*.ts --fix",
    "prestart": "rm -rf build/ && tsc",
    "start": "serverless offline",
    "test": "nyc mocha",
    "predeploy": "rm -rf build/",
    "deploy": "sls deploy -v",
    "prepush": "yarn test"
  },
  "nyc": {
    "function": "70",
    "branches": "76",
    "lines": "40",
    "check-coverage": "true",
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "test/**"
    ],
    "extension": [
      ".ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/allied-modalgr/serverless-template.git"
  },
  "keywords": [
    "API",
    "Serverless",
    "Template"
  ],
  "author": "ModalGR team",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/allied-modalgr/serverless-template/issues"
  },
  "homepage": "https://gitlab.com/allied-modalgr/serverless-template#readme",
  "dependencies": {
    "@types/sinon": "^7.0.13",
    "aws-sdk": "^2.528.0",
    "axios": "^0.19.0",
    "bunyan": "^1.8.12",
    "dotenv": "^8.0.0",
    "html-to-text": "^5.1.1",
    "knex": "^0.19.1",
    "moment": "^2.24.0",
    "mysql": "^2.17.1",
    "mysql2": "^1.6.5",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "xml-to-json-stream": "^1.1.0",
    "yarn": "^1.17.3"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/chai-http": "4.2.0",
    "@types/compression": "0.0.36",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.16.1",
    "@types/mocha": "5.2.6",
    "@types/request": "2.48.1",
    "@typescript-eslint/eslint-plugin": "1.7.0",
    "@typescript-eslint/parser": "1.7.0",
    "chai": "4.2.0",
    "chai-http": "4.2.1",
    "eslint": "5.16.0",
    "eslint-plugin-import": "2.17.2",
    "gulp": "^4.0.2",
    "gulp-shell": "0.7.0",
    "husky": "^1.3.1",
    "mocha": "6.1.4",
    "nyc": "14.0.0",
    "serverless-offline": "4.9.2",
    "serverless-plugin-tracing": "2.0.0",
    "serverless-plugin-typescript": "1.1.5",
    "serverless-webpack": "5.2.0",
    "ts-node": "8.0.3",
    "typescript": "3.3.4000"
  }
}
